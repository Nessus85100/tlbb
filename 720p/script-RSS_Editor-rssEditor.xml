<window>
	<defaultcontrol>100</defaultcontrol>
	<controls>
  <control type="button" id="100">
		<include>TopButton</include>
    <onfocus>SetFocus(13)</onfocus>
    <animation effect="fade" start="0" end="100" time="0" delay="50">Focus</animation>
  </control>
	<control type="group">
		<animation type="WindowOpen" condition="Window.IsVisible(script-RSS_Editor-setEditor.xml)" reversible="false">
			<effect type="fade" start="0" end="100" time="0" delay="300" />
		</animation>    
		<control type="group">
			<visible>!Window.IsVisible(virtualkeyboard) + !Window.IsVisible(numericinput)</visible>
			<include>DialogAnimation</include>
			<control type="image">
				<description>background image</description>
				<include>FullscreenDimensions</include>
				<texture background="true">dialogs/other/bg_rss.png</texture>
			</control>
			<control type="label" id="2">
				<description>header label</description>
				<posx>305</posx>
				<posy>262</posy>
				<width>670</width>
				<height>50</height>
				<font>FontSize_36</font>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>99000000</shadowcolor>
				<scroll>true</scroll>
				<scrollspeed>20</scrollspeed>
			</control>
			<control type="label" id="3">
				<description>List label</description>
				<font>-</font>
				<visible>false</visible>
			</control>
	
			<control type="group" id="9000">
			 <control type="list" id="10">
				<description>Feeds List Control</description>
				<posx>328</posx>
				<posy>317</posy>
				<width>460</width>
				<height>154</height>
				<onup>10</onup>
				<ondown>10</ondown>
				<onleft>10000</onleft>
				<onright>9001</onright>
				<scrolltime>200</scrolltime>
				<include>VisibleFadeEffect</include>
				<itemlayout height="51" width="460">
					<control type="label">
						<posx>10</posx>
						<posy>-1</posy>
						<width>440</width>
						<height>47</height>
						<font>FontSize_20</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>77white</textcolor>
						<selectedcolor>selected</selectedcolor>
						<info>ListItem.Label</info>
					</control>
				</itemlayout>
				<focusedlayout height="51" width="460">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>460</width>
						<height>47</height>
						<texture>buttons/settings_fo.png</texture>
						<visible>Control.HasFocus(10)</visible>
	          <include>FocusVisibleFadeEffect</include>
				  </control>
					<control type="label">
						<posx>10</posx>
						<posy>-1</posy>
						<width>440</width>
						<height>47</height>
						<font>FontSize_20</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<selectedcolor>selected</selectedcolor>
						<info>ListItem.Label</info>
						<visible>Control.HasFocus(10)</visible>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>-1</posy>
						<width>440</width>
						<height>47</height>
						<font>FontSize_20</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>77white</textcolor>
						<selectedcolor>selected</selectedcolor>
						<info>ListItem.Label</info>
						<visible>!Control.HasFocus(10)</visible>
					</control>
				</focusedlayout>
			 </control>
		
			 <control type="group" id="9001">
				<posx>792</posx>
				<posy>317</posy>
				<control type="button" id="13">
					<description>Add Button</description>
					<posy>0</posy>
					<width>160</width>
					<onup>11</onup>
					<ondown>14</ondown>
					<onleft>10</onleft>
					<onright>10000</onright>
					<label>15019</label>
		      <align>center</align>
	        <include>DialogsButton</include>
				</control>
				<control type="button" id="14">
					<description>Remove Button</description>
					<posy>51</posy>
					<width>160</width>
					<onup>13</onup>
					<ondown>11</ondown>
					<onleft>10</onleft>
					<onright>10000</onright>
					<label>1210</label>
		      <align>center</align>
	        <include>DialogsButton</include>
				</control>
				<control type="button" id="11">
					<description>Change Set Button</description>
					<posy>102</posy>
					<width>160</width>
					<onup>14</onup>
					<ondown>13</ondown>
					<onleft>10</onleft>
					<onright>10000</onright>
		      <align>center</align>
	        <include>DialogsButton</include>
					<onclick>PreviousMenu</onclick>
				</control>
			 </control>
			</control>
	
		  <control type="group" id="10000">
				<posx>330</posx>
				<posy>460</posy>
	      <!-- Buttons -->
				<control type="grouplist" id="10001">
					<posx>0</posx>
					<posy>0</posy>
					<width>0</width>
					<height>0</height>
					<itemgap>0</itemgap>
					<align>left</align>
					<orientation>horizontal</orientation>
					<onleft>10001</onleft>
					<onright>10001</onright>
					<onup>9000</onup>
					<ondown>9000</ondown>
					<control type="button" id="18">
						<description>Save</description>
					  <label>190</label>
					  <include>TopButton</include>
					</control>
					<control type="button" id="19">
						<description>Cancel</description>
					  <label>222</label>
					  <include>TopButton</include>
					</control>
				</control>
	      <!-- Buttons Labels -->
				<control type="grouplist">
					<posx>0</posx>
					<posy>0</posy>
					<height>70</height>
					<width>552</width>
					<itemgap>30</itemgap>
					<align>left</align>
					<orientation>horizontal</orientation>
					<control type="label">
						<description>Save Label</description>
						<height>62</height>
						<width min="10" max="552">auto</width>
						<font>FontSize_20</font>
						<textcolor>white</textcolor>
						<shadowcolor>99000000</shadowcolor>
				    <align>left</align>
				    <aligny>center</aligny>
						<scroll>false</scroll>
	          <label>$INFO[Control.GetLabel(18),․  ]</label>
						<animation effect="fade" start="100" end="30" time="200" tween="sine" easing="inout" condition="!Control.HasFocus(18)" reversible="true">Conditional</animation>
	          <animation effect="zoom" start="100" end="95" time="200" tween="sine" easing="inout" center="15,0" condition="!Control.HasFocus(18)" reversible="true">Conditional</animation>
					</control>
					<control type="label">
						<description>Cancel Label</description>
						<height>62</height>
						<width min="10" max="552">auto</width>
						<font>FontSize_20</font>
						<textcolor>white</textcolor>
						<shadowcolor>99000000</shadowcolor>
				    <align>left</align>
				    <aligny>center</aligny>
						<scroll>false</scroll>
	          <label>$INFO[Control.GetLabel(19),․  ]</label>
						<animation effect="fade" start="100" end="30" time="200" tween="sine" easing="inout" condition="!Control.HasFocus(19)" reversible="true">Conditional</animation>
	          <animation effect="zoom" start="100" end="95" time="200" tween="sine" easing="inout" center="15,0" condition="!Control.HasFocus(19)" reversible="true">Conditional</animation>
					</control>
				</control>
		  </control>
	
		</control>
	</control>
	</controls>
</window>