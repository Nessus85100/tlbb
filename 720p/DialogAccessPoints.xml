<window id="141">
	<defaultcontrol always="true">100</defaultcontrol>
	<controls>
	<control type="button" id="100">
		<include>TopButton</include>
    <onfocus>SetFocus(3)</onfocus>
    <animation effect="fade" start="0" end="100" time="0" delay="50">Focus</animation>
  </control>
	 <control type="group">
	  <include>DialogAnimation</include>
		<visible>!Window.IsActive(yesnodialog) + !Window.IsActive(busydialog) + !Window.IsActive(VirtualKeyboard)</visible>
		<control type="image">
			<description>background image</description>
			<include>FullscreenDimensions</include>
			<texture background="true">dialogs/selectdialog/bg_select.png</texture>
		</control>
		<control type="label" id="1">
			<description>header label</description>
			<posx>340</posx>
			<posy>185</posy>
			<width>600</width>
			<font>FontSize_36</font>
			<textcolor>white</textcolor>
			<align>center</align>
			<aligny>center</aligny>
			<scroll>true</scroll>
			<scrollspeed>20</scrollspeed>
			<label>1046</label>
		</control>
		<control type="list" id="3">
			<posx>363</posx>
			<posy>229</posy>
			<width>552</width>
			<height>306</height>
			<onup>3</onup>
			<ondown>3</ondown>
			<onleft>close</onleft>
			<onright>close</onright>
			<scrolltime tween="Cubic" easing="Out">250</scrolltime>
			<itemlayout height="51" width="552">
				<control type="image">
					<posx>6</posx>
					<posy>9</posy>
					<width>28</width>
					<height>28</height>
					<colordiffuse>55ffffff</colordiffuse>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<texture>dialogs/accesspoints/ap-lock.png</texture>
					<visible>!IsEmpty(ListItem.Property(encryption))</visible>
				</control>
				<control type="image">
					<posx>510</posx>
					<posy>4</posy>
					<width>40</width>
					<height>40</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<texture>dialogs/accesspoints/$INFO[ListItem.Property(state)].png</texture>
				</control>
				<control type="image">
					<posx>467</posx>
					<posy>14</posy>
					<width>40</width>
					<height>22</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
				  <texture>dialogs/accesspoints/ap-signal$INFO[ListItem.Property(signal)].png</texture>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>0</posy>
					<width>420</width>
					<height>48</height>
					<font>FontSize_20</font>
					<textcolor>77white</textcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
	        <animation effect="slide" end="30,0" time="200" reversible="false" condition="!IsEmpty(ListItem.Property(encryption))">Conditional</animation>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>552</width>
					<height>47</height>
					<texture>dialogs/selectdialog/selected.png</texture>
    			<visible>ListItem.IsSelected</visible>
				</control>
			</itemlayout>
			<focusedlayout height="51" width="552">
			 <control type="group">
	      <animation effect="slide" start="0,35" end="0,0" time="350" reversible="false" tween="back" easing="out" condition="Container(3).OnPrevious">focus</animation>
	      <animation effect="slide" start="0,-35" end="0,0" time="350" reversible="false" tween="back" easing="out" condition="Container(3).OnNext">focus</animation>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>552</width>
					<height>47</height>
					<texture>buttons/settings_fo.png</texture>
					<visible>Control.HasFocus(3)</visible>
					<include>VisibleFadeEffect</include>
				</control>
				<control type="image">
					<posx>6</posx>
					<posy>9</posy>
					<width>28</width>
					<height>28</height>
					<colordiffuse>ffcccccc</colordiffuse>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<texture>dialogs/accesspoints/ap-lock.png</texture>
					<visible>!IsEmpty(ListItem.Property(encryption))</visible>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>0</posy>
					<width>420</width>
					<height>48</height>
					<font>FontSize_20</font>
					<textcolor>white</textcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
	        <animation effect="slide" end="30,0" time="200" reversible="false" condition="!IsEmpty(ListItem.Property(encryption))">Conditional</animation>
				</control>
				<control type="image">
					<posx>510</posx>
					<posy>4</posy>
					<width>40</width>
					<height>40</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
					<texture>dialogs/accesspoints/$INFO[ListItem.Property(state)].png</texture>
				</control>
				<control type="image">
					<posx>467</posx>
					<posy>14</posy>
					<width>40</width>
					<height>22</height>
					<aspectratio align="center" aligny="center">keep</aspectratio>
				  <texture>dialogs/accesspoints/ap-signal$INFO[ListItem.Property(signal)].png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>552</width>
					<height>51</height>
					<texture>dialogs/selectdialog/selected.png</texture>
    			<visible>ListItem.IsSelected</visible>
				</control>
			 </control>
			</focusedlayout>
		</control>
	 </control>
	</controls>
</window>
